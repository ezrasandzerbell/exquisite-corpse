<section class="c_stories--sect">
  <header class="c_stories--header">
    {{link-to "New Story" "stories.new" data-test-action="new-story" class="btn"}}
  </header>
  <ul class="c_stories-list">
    {{#each model as |story|}}
      {{story-listing story=story data-test-story-id=story.id}}
    {{/each}}
  </ul>
</section>
